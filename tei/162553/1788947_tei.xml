      <lb facs="#facs_1_region_1706948092982_542_tl_2" n="N002"/>D
      <lb facs="#facs_1_region_1706948092982_542_tl_3" n="N003"/>Freie
      <lb facs="#facs_1_region_1706948092982_542_tl_4" n="N004"/>1890.</ab><ab facs="#facs_1_region_1706948094432_545" type="">
